const lista = []

document.querySelectorAll('tr').forEach(row => {
    // Seleciona os links de ação dentro da última coluna da linha
    const actionLinks = row.querySelectorAll('td:nth-child(6) .icon-actions a');

    actionLinks.forEach(actionLink => {
        // Verifica se o link contém o texto "Ver Detalhes"
        if (!actionLink.getAttribute('href').includes("DownloadLot")) {
            // Obtém o valor do href do link de "Ver Detalhes"
            lista.push("https://www.meutibi.com.br"+actionLink.getAttribute('href'));
        }
    });
});

window.location.href=lista[0]


var listaemtransporte=[]
var pedidosEmTransporte = document.querySelectorAll('.pedido-em-transporte');

// Itera sobre as linhas encontradas
pedidosEmTransporte.forEach(function(statusSpan) {
    // Encontra a linha (tr) mais próxima do span do status
    var tr = statusSpan.closest('tr');

    // Encontra o código de rastreio dentro dessa linha
    var codigoRastreio = tr.querySelector('.edit').textContent.trim();

    // Imprime o código de rastreio
    listaemtransporte.push("https://www.linkcorreios.com.br/" + codigoRastreio);
});

window.location.href=listaemtransporte[0]

window.history.back()